{
    "metadata": [
        {
            "src": [
                {
                    "files": [
                        "src/**/*.csproj"
                    ],
                    "exclude": [
                        "**/bin/**",
                        "**/obj/**"
                    ]
                }
            ],
            "dest": "docs/api-temp",
            "disableGitFeatures": false,
            "disableDefaultFilter": false,
            "properties": {
                "TargetFramework": "net10.0"
            }
        }
    ],
    "build": {
        "content": [
            {
                "files": [
                    "docs/api-temp/*.yml",
                    "docs/api-temp/index.md"
                ],
                "src": ".",
                "dest": "api"
            },
            {
                "files": [
                    "docs/**/*.md",
                    "README.md"
                ],
                "exclude": [
                    "docs/api-temp/**"
                ],
                "src": ".",
                "dest": "."
            },
            {
                "files": [
                    "examples/**/*.md"
                ],
                "src": ".",
                "dest": "."
            }
        ],
        "resource": [
            {
                "files": [
                    "images/**",
                    "docs/images/**",
                    "**/*.png",
                    "**/*.jpg",
                    "**/*.gif"
                ],
                "exclude": [
                    "docs/api-temp/**",
                    "**/bin/**",
                    "**/obj/**"
                ]
            }
        ],
        "overwrite": [
            {
                "files": [
                    "docs/api-overrides/**/*.md"
                ],
                "src": ".",
                "dest": "."
            }
        ],
        "dest": "_site",
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "template": [
            "default",
            "modern"
        ],
        "postProcessors": [],
        "markdownEngineName": "markdig",
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false,
        "disableGitFeatures": false,
        "globalMetadata": {
            "_appTitle": "GraphModel Documentation",
            "_appName": "GraphModel",
            "_appFooter": "GraphModel - A powerful .NET graph database library",
            "_enableSearch": true,
            "_enableNewTab": true,
            "_disableContribution": false,
            "_gitContribute": {
                "repo": "https://github.com/savasp/graphmodel",
                "branch": "main"
            },
            "_gitUrlPattern": "github"
        }
    }
}